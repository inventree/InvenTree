          type: integer
          description: Select part to build
        project_code:
          type: integer
          nullable: true
          description: Project code for this build order
        project_code_detail:
          allOf:
          - $ref: '#/components/schemas/ProjectCode'
          readOnly: true
        overdue:
          type: boolean
          readOnly: true
        reference:
